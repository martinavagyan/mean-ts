{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "double",
      "avoid-escape",
      "avoid-template"
    ],
    "ordered-imports": false,
    "no-console": [false, "log", "error"],
    "jsdoc-format": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "no-var-requires": false
  },
  "rulesDirectory": []
}